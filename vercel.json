{
  "version": 2,

  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "installCommand": "npm install --legacy-peer-deps",

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],

  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }

  },
  "regions": [
    "iad1"
  ],
  "framework": "nextjs"
}

  }


